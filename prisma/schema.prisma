generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String   @id @default(uuid())
  email    String   @unique
  password String
  role     String
  createdAt DateTime @default(now())
}

model Client {
  id        String   @id @default(uuid())
  nom       String
  prenom    String?
  email     String?  @unique
  telephone String?  @unique
  site      String
  type      String
  createdAt DateTime @default(now())
}

model Import {
  id         String   @id @default(uuid())
  filename   String
  totalRows  Int
  validRows  Int
  invalidRows Int
  createdAt  DateTime @default(now())
}
